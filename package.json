{
  "name": "ecommerce",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build ",
    "format": "prettier --ignore-path gitignore --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:local": "cross-env NODE_ENV=local npm run start",
    "start:dev": "cross-env NODE_ENV=development nest build --webpack --webpackPath webpack-hmr.config.js --watch",
    "start:debug": "cross-env NODE_ENV=development nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "dotenv -e .env.test jest",
    "test:watch": "dotenv -e .env.test jest --watchAll",
    "test:cov": "dotenv -e .env.test jest --coverage",
    "test:debug": "cross-env NODE_ENV=test node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "cross-env NODE_ENV=test jest --config ./test/jest-e2e.json",
    "prisma:seed": "dotenv -e .env.development -- npx prisma db seed",
    "migrate:dev": "dotenv -e .env.development -- npx prisma migrate dev --name postgres-update-dev",
    "migrate:test": "dotenv -e .env.test -- npx prisma migrate dev --name postgres-update-test",
    "reset:dev": "dotenv -e .env.development -- npx prisma migrate reset",
    "coverage": "jest run --coverage",
    "test:dbPush": "dotenv -e .env.test -- npx prisma db push --accept-data-loss",
    "prisma:validate": "dotenv -e .env.development -- npx prisma validate"
  },
  "dependencies": {
    "@casl/ability": "^6.1.1",
    "@casl/prisma": "^1.1.0",
    "@jest-mock/express": "^2.0.1",
    "@nestjs/common": "^9.0.3",
    "@nestjs/config": "^2.2.0",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.0.3",
    "@nestjs/swagger": "^6.0.4",
    "@prisma/client": "^4.3.1",
    "@types/cookie-session": "^2.0.44",
    "@types/passport-facebook": "^2.1.11",
    "algoliasearch": "^4.14.2",
    "body-parse": "^0.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cookie-session": "^2.0.0",
    "cross-env": "^7.0.3",
    "crypto-js": "^4.1.1",
    "dotenv-cli": "^6.0.0",
    "express": "^4.18.1",
    "jsonwebtoken": "^8.5.1",
    "node-mocks-http": "^1.11.0",
    "nodemon": "^2.0.19",
    "npm-check-updates": "^15.2.6",
    "passport": "^0.6.0",
    "passport-apple": "^2.0.1",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth2": "^0.2.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "sinon": "^14.0.0",
    "stripe": "^9.14.0",
    "swagger-ui-express": "^4.4.0",
    "ts-jest": "^28.0.8",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/plugin-syntax-decorators": "^7.18.6",
    "@compodoc/compodoc": "^1.1.19",
    "@faker-js/faker": "^7.4.0",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.1",
    "@nestjs/testing": "^9.0.11",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.6",
    "@types/node": "^16.0.0",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "c8": "^7.12.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "28.0.3",
    "jest-mock-extended": "^2.0.7",
    "prettier": "^2.3.2",
    "prettier-plugin-prisma": "^3.14.0",
    "prisma": "^4.3.1",
    "run-script-webpack-plugin": "^0.1.1",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.0.0",
    "typescript": "^4.3.5",
    "vitest": "^0.21.1",
    "vitest-mock-extended": "^0.1.13",
    "webpack": "^5.73.0",
    "webpack-node-externals": "^3.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "coverageReporters": [
      "text",
      "html",
      "json"
    ],
    "testPathIgnorePatterns": [
      "./node_modules/",
      "./dist",
      "./documentation"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "collectCoverage": false,
    "verbose": true
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  }
}
